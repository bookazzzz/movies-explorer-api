{
  "env": {
      "browser": true,
      "es2021": true,
      "es6": true,
      "node": true,
      "amd": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 2021,
      "sourceType": "module"
  },
  "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"]}]

  }
}